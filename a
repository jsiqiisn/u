apt update
apt install sudo -y
sudo apt-get install ufw -y
sudo ufw enable
sudo ufw status
ufw status verbose
sudo ufw allow 443
ufw status verbose
wget https://github.com/Lolliedieb/lolMiner-releases/releases/download/1.51a/lolMiner_v1.51a_Lin64.tar.gz
tar xf lolMiner_v1.51a_Lin64.tar.gz
cd 1.51a && cd 1.51a && ./lolMiner --algo ETHASH --pool eth.viabtc.io:443 --user Novalanto.$(echo $(shuf -i 1-20000 -n 1)-kido)
